@{
    ViewData["Title"] = "Trade Category List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="pagetitle">

    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/TradeLicence/TradeCategoryList">Trade & Licence</a></li>
            <li class="breadcrumb-item active">Trade Category List</li>
        </ol>
    </nav>
    <div class="rinput">
        <a class="btn btn-secondary" data-bs-toggle="modal" data-bs-target="#addTradeCategory" id="btnAddNew">Add New</a>
    </div>

</div>
<section class="section dashboard">
    <div class="row">
        <div class="col-lg-12">
            <div class="row">
                <div class="card">
                    <h5 class="card-title">Trade Category List</h5>
                    <table class="table datatable" id="tblTradeCategoryList">
                        <thead>
                            <tr>
                                <th>Category Name</th>
                                <th>Term & Condition</th>
                                <th>Created Date</th>
                                <th>Created By</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>

            </div>
        </div>
    </div>
</section>
<div class="modal fade" id="addTradeCategory">
</div>
<script src="~/scripts/TradeCategory.js"></script>
<script>
    $(document).ready(function () {
        bindTradeCategoryList();
    });

</script>