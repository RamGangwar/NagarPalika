@{
    ViewData["Title"] = "Trade Registration List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .textright {
        text-align: right;
    }
    .textcenter {
        text-align: center;
    }
    .small{
        font-size:x-small;
    }
</style>
<div class="pagetitle">

    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/TradeLicence/TradeRegistrationList">Trade Licence</a></li>
            <li class="breadcrumb-item active">Trade Registration List</li>
        </ol>
    </nav>
    <div class="rinput">
        <a class="btn btn-secondary" href="/TradeLicence/AddEditTradeRegistration" target="_blank" id="btnAddNew">Add New</a>
    </div>

</div>
<section class="section dashboard">
    <div class="row">
        <div class="col-lg-12">
            <div class="row">
                <div class="card">
                    <h5 class="card-title">Trade Registration List</h5>
                    <table class="table datatable" id="tblTradeRegistrationList">
                        <thead>
                            <tr style="font-size:x-small">
                                <th>Zone</th>
                                <th>Ward</th>
                                <th>Locality</th>
                                <th>Trade Category</th>
                                <th>Trade Sub Category</th>
                                <th>Trade Code</th>
                                <th>Application Type</th>
                                <th>Owner Name</th>
                                <th>Father Name</th>
                                <th>Mobile No</th>
                                <th>Gender</th>
                                <th>Trade Name</th>
                                <th>Trade Address</th>                                
                                <th>ARV Effected Date</th>
                                <th>Arrear</th>
                                <th>Arrear From</th>
                                <th>Arrear Till</th>
                                <th>Created Date</th>
                                <th>Created By</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>

            </div>
        </div>
    </div>
</section>
<div class="modal fade" id="addTradeRegistration">
</div>
<script src="~/scripts/TradeRegistration.js"></script>
<script>
    $(document).ready(function () {
        bindTradeRegistrationList();
    });

</script>