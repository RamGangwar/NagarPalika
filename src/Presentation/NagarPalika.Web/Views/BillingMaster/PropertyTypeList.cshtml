                            <tr>
                                <th>Grade </th>
                                <th>PropertyType</th>
                                <th>Factor</th>
                                <th>Created Date</th>
                                <th>Created By</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>

            </div>
        </div>
    </div>
</section>
<div class="modal fade" id="addPropertyType">
</div>
<script src="~/scripts/PropertyType.js"></script>
<script>
    $(document).ready(function () {
        bindPropertyTypeList();
    });

</script>